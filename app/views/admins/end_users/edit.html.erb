<div class="container">
	<div class ="row">
		<div class= "col">
			<h2>会員編集画面</h2>

			<%= form_for [:admins, @end_user] do |f| %>

			<h4>会員ID<%= @end_user.id %></h4>
			<h4>氏名</h4>
			<%= f.text_field :first_name %>
			<%= f.text_field :last_name %>
			<h4>フリガナ</h4>
			<%= f.text_field :first_kana_name %>
			<%= f.text_field :last_kana_name %>
			<h4>郵便番号</h4>
			<%= f.text_field :postcode %>
			<h4>住所</h4>
			<%= f.text_field :address %>
			<h4>電話番号</h4>
			<%= f.text_field :phone_number %>
			<h4>メールアドレス</h4>
			<%= f.text_field :email %>
			<h4>会員ステータス</h4>
            <%= f.label :is_deleted, "有効" %>
            <%= f.radio_button :is_deleted, true %>
            <%= f.label :is_deleted, "退会済み" %>
			<%= f.radio_button :is_deleted, false %>
			<%= f.submit '変更を保存する' %>

			<% end %>
		</div>
	</div>
</div>