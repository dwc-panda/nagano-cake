<div class="container">
	<div class="row">
		<div class="col-xs-3 col-xs-offset-1">
			<h2>会員一覧画面</h2>
		</div>
	</div>
</div>
<div class="row">
	<div class="col-xs-6 col-xs-offset-3">
		<table class="table">
			<thead>
				<tr class="active">
					<th>会員ID</th>
					<th>氏名</th>
					<th>メールアドレス</th>
					<th>ステータス</th>
				</tr>
			</thead>
			<tbody>
				<% @end_users.each do |end_user| %>
				<tr>
					<td><%= end_user.id %></td>
					<td><%= link_to end_user.first_name + end_user.last_name,admins_end_user_path(end_user)  %></td>
					<td><%= end_user.email %></td>
					<td><%= end_user.is_deleted %></td>
				</tr>
				<% end %>
			</tbody>
		</table>
	</div>
</div>