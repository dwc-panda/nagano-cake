<div class="container">
	<div class ="row">
		<div class= "col">
			<h4>会員一覧画面</h4>
			<table class="table">
				<thead>
					<tr>
						<th>会員ID</th>
						<th><%= link_to "氏名", admins_end_user_path(@end_users) %></th>
						<th></th>
						<th>メールアドレス</th>
						<th>ステータス</th>
					</tr>
				</thead>
				<tbody>
					<% @end_users.each do |end_user| %>
					<tr>
						<td><%= end_user.id %></td>
						<%= link_to end_user_path(@end_users) do %>
						<td><%= end_user.first_name %><%= end_user.last_name %><td>
						<% end %>
						<td><%= end_user.email %></td>
						<td><%= end_user.is_deleted %></td>
					</tr>
						<% end %>
					</tbody>
				</table>
			</div>
		</div>
	</div>