<div class="container">
	<div class="row">
		<div class="col">
			<h1>注文一覧画面</h1>

	  <table class="table">
	  	<thead>
	  	  <tr>
	  	  	<th>購入日時</th>
	  	  	<th>購入者</th>
	  	  	<th>注文個数</th>
	  	  	<th>注文ステータス</th>
	  	  </tr>
	  	</thead>
	  	<tbody>
	  	<% @orders.each do |order| %>
	  	  <tr>
	  	  	<td><%= order.created_at.strftime('%Y/%m/%d %H:%M:%S') %></td>
	  	  	<td><%= order.end_user.first_name %> <%= order.end_user.last_name %></td>
	  	  	<td><%= order.order_details.quantity %></td>
	  	  	<td><%= order.order_status %></td>
	  	  </tr>
	  	<% end %>
	  </tbody>
	</table>
